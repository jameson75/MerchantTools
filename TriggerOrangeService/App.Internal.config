<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="OrangeEntities"
        connectionString="metadata=res://*/Data.Model.csdl|res://*/Data.Model.ssdl|res://*/Data.Model.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=localhost\MSSQLSERVER14;initial catalog=Orange_Internal;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="CipherPark.TriggerOrange.Service.OrangeWCFService" behaviorConfiguration="TriggerOrangeWCFServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="net.pipe://localhost/TriggerOrangeWCF/Internal/service" xdt:Transform="Replace"/>
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>